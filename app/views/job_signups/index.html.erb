<p id="notice"><%= notice %></p>

<h1>Listing Job Signups</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Job</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_signups.each do |job_signup| %>
      <tr>
        <td><%= job_signup.user %></td>
        <td><%= job_signup.job %></td>
        <td><%= link_to 'Show', job_signup %></td>
        <td><%= link_to 'Edit', edit_job_signup_path(job_signup) %></td>
        <td><%= link_to 'Destroy', job_signup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job signup', new_job_signup_path %>
